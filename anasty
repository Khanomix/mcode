random;tof install
printf "\n\n\n \033[1;92m Installing req.\n\n\n"
random;pip3 install --no-cache-dir -r requirements.txt
pip3 install --force-reinstall speedtest-cli
pip3 install yt-dlp==2023.10.13
random;tof starting
printf "\n\n  Now bot start \n\n"
ls
bash start.sh
